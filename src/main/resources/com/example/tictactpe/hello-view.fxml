                        <Insets />
                     </GridPane.margin>
                  </TextField>
                  <TextField fx:id="playerOName" onInputMethodTextChanged="#playerOChange" prefWidth="260.0" GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="NEVER" GridPane.rowIndex="1" />
